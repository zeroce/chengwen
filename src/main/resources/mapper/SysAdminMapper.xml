  <select id="findPerDetailBy" parameterType="Map" resultMap="SysAdminWithRolePermissionMap">
    SELECT
    a.*,
    r.id as role_id,
    r.name as role_name,
    p.code as permission_code
    FROM sys_admin a
    LEFT JOIN sys_admin_roles ar ON a.id = ar.sys_admin_id
    LEFT JOIN sys_roles r ON ar.sys_roles_id = r.id
    LEFT JOIN sys_roles_permission rp ON r.id = rp.sys_roles_id
    LEFT JOIN sys_permission p ON rp.sys_permission_id = p.id
    <where>
      <if test="id != null">a.id = #{id}</if>
      <if test="username != null">a.username = #{username}</if>
    </where>
  </select>

